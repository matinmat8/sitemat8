<script type="text/javascript">
    var auth2; // The Sign-In object.
    var googleUser; // The current user.

    /**
     * Calls startAuth after Sign in V2 finishes setting up.
     */
    var gOAuthAppStart = function() {
        gapi.load('auth2', initSigninV2);
    };

    /**
     * Initializes Signin v2 and sets up listeners.
     */
    var initSigninV2 = function() {
        auth2 = gapi.auth2.init({
          client_id: '891930018278-nstjhv5kqto07g2j011k5us0kujtnupp.apps.googleusercontent.com',
          scope: 'profile'
        });

                    gapi.signin2.render('google-signin2', {
                'scope': 'profile email',
                'width': 300,
                'height': 50,
                'longtitle': true,
                'theme': 'dark',
                'title' : 'login apb',
                'onsuccess': gOnSuccess,
                'onfailure': gOnFailure
            });
            setTimeout(function(){
                $("#google-signin2 div div span span:first").text("ورود با حساب گوگل");
                $("#google-signin2 div div span span:last").text("ورود انجام شد!");
            }, 1000);


            }

            function gOnSuccess(googleUser) {
            authapp.loading = true;
            // $(".modal.auth .dimmer").addClass("active");
            var id_token = googleUser.getAuthResponse().id_token;
            // console.log(id_token);
            // return;
            $.post(ROOT_URL+"authapi/googleoauth", {'id_token' : id_token}, function(res) {
                authapp.loading = false;
                if(res.type != 'false'){
                    authapp.operation_type = "redirect";
                    authapp.successMsg = 'خوش اومدی!';
                    redirect('dashboard');
                }
            },'json');
            // console.log('Logged in as: ' + googleUser.getBasicProfile().getName());
        }

        function gOnFailure(error) {
            console.log(error);
        }
    
    function signOut() {
        if(typeof gapi != 'undefined'){
            var auth = gapi.auth2.getAuthInstance();
            if(auth.isSignedIn.get() == true){
                auth.disconnect().then(function () {
                    redirect('user/logout');
                });
            }else{
                redirect('user/logout');
            }
        }else{
            redirect('user/logout');
        }
    }
</script>
